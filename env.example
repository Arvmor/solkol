# Solana RPC Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Conservative Rate Limiting Settings (to prevent 429 errors)
SLOT_POLL_INTERVAL=5000
MAX_REQUESTS_PER_SECOND=3
REQUEST_DELAY=500
RATE_LIMIT_BACKOFF_MULTIPLIER=3.0
MAX_RATE_LIMIT_DELAY=60000

# Slot Processing Limits
MAX_SLOTS_PER_BATCH=2
SLOT_PROCESSING_DELAY=1000

# Retry Configuration
MAX_RETRIES=5
RETRY_DELAY=1000

# Logging
LOG_LEVEL=info
ENABLE_PERFORMANCE_LOGS=false 